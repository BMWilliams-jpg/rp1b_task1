#Computing GC Content

import os #import module to allow access to files on desktop

with open((os.path.expanduser('~/Desktop/rosalind_gc.txt')), 'r') as f:
    file = f.read().replace('\n', '').split('>')[1:]
#create list containing file contents

def get_gc_content(seq):
    return ((seq.count('G') + seq.count('C'))/len(seq)) * 100
#function to compute GC content

dict = {}
for string in file:
    ID = string[:13]
    dict[ID] = get_gc_content(string[13:])
#dictionary created to store identifiers and appropriate GC content

print(max(dict, key=dict.get))
print(max(dict.values()))
#printing output in desired format
